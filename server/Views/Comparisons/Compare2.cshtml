@model List<User>

@{
    ViewData["Title"] = "Home Page";
}

<div class="comparison-container border bg-light p-3">
    <div class="d-flex justify-content-center">
        <div class="comparison-card bg-success text-white rounded m-3" style="width:300px; height:300px">
            <h2>Candidate Order from Model</h2>
            @foreach (User candidate in Model)
            {
                <p>@candidate.FirstName</p>
            }
        </div>
        <div class="comparison-card bg-success text-white rounded m-3" style="width:300px; height:300px">
            <h2>Candiate Order from ViewBag</h2>
        </div>
    </div>
    <div class="d-flex justify-content-center">
        <div class="comparison-card bg-success text-white rounded m-3" style="width:300px; height:300px">
            <h2>Candiate Info #3</h2>
        </div>
        <div class="comparison-card bg-success text-white rounded m-3" style="width:300px; height:300px">
            <h2>Candiate Info #4</h2>
        </div>
    </div>         
</div>